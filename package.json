{
  "name": "secure-audio-streaming",
  "version": "1.0.0",
  "description": "Secure audio streaming application with DRM protection",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "prisma migrate dev",
    "seed": "node prisma/scripts/seed.js",
    "setup": "npm run migrate && npm run seed",
    "generate": "prisma generate",
    "client": "cd client && npm start",
    "install-all": "npm install && cd client && npm install",
    "dev:all": "concurrently \"npm run dev\" \"npm run client\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@prisma/client": "^5.0.0",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.0.1",
    "fluent-ffmpeg": "^2.1.3",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "react-player": "^3.3.1",
    "react-router-dom": "^7.8.1",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.0.0"
  }
}
